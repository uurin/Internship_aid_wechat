/* pages/home/checkIn/checkIn.wxss */
page{
  background: #f7f7f7;
}


.container-checkIn {
  margin: 3rem;
  display: flex;
  justify-content: center;
}
.btn-checkIn {
  width: 100px;
  height: 100px;
  border-radius: 100px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.btn-checkIn text {
  font-size: 1.2rem;
  color: #fff;
}
.style-uncheckedIn {
  background: #40a9ff;
}
.style-checkedIn {
  background: #bdddf7;
}